{
  "name": "mdtocs",
  "version": "1.0.5",
  "description": "Markdown table of contents generator.",
  "author": "Mark <mark@remarkablemark.org>",
  "main": "cjs/index.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:umd",
    "build:cjs": "tsc --declaration --outDir cjs",
    "build:umd": "rollup --config",
    "clean": "rm -rf cjs umd",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:tsc": "tsc --noEmit",
    "prepare": "husky",
    "prepublishOnly": "npm run lint && npm test && npm run clean && npm run build",
    "size": "npm run build && size-limit",
    "test": "CI=true jest --ci --colors --coverage",
    "test:watch": "jest --watch"
  },
  "homepage": "https://remarkablemark.org/mdtocs/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remarkablemark/mdtocs.git"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/mdtocs/issues"
  },
  "keywords": [
    "mdtocs",
    "md",
    "toc",
    "markdown",
    "table",
    "of",
    "contents",
    "generator"
  ],
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/compat": "2.0.0",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.1",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.3.0",
    "@size-limit/preset-small-lib": "12.0.0",
    "@types/jest": "30.0.0",
    "@typescript-eslint/eslint-plugin": "8.48.1",
    "@typescript-eslint/parser": "8.48.1",
    "eslint": "9.39.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-tsdoc": "0.5.0",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "lint-staged": "16.2.7",
    "prettier": "3.7.3",
    "rollup": "4.53.3",
    "size-limit": "12.0.0",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.9.3"
  },
  "files": [
    "cjs/",
    "umd/"
  ],
  "license": "MIT"
}
