{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "plugins": ["@typescript-eslint", "prettier", "tsdoc"],
  "rules": {
    "no-console": "error",
    "no-debugger": "error",
    "prettier/prettier": "error",
    "tsdoc/syntax": "error"
  }
}
